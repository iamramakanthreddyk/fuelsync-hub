{
  "name": "fuelsync-hub-backend",
  "version": "1.0.0",
  "description": "Backend for FuelSync Hub - Multi-tenant Fuel Station Management Platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "db:env": "ts-node db/setup-local-env.ts",
    "db:check": "ts-node db/check-connection.ts",
    "db:debug": "ts-node db/debug-schema.ts",
    "db:schema": "ts-node db/setup-db.ts",
    "db:schema:chunks": "ts-node db/apply-schema-in-chunks.ts",
    "db:tenant-schemas": "ts-node db/create-tenant-schema.ts",
    "db:copy-users": "ts-node db/copy-users-to-tenant-schemas.ts",
    "db:verify-schemas": "ts-node db/verify-tenant-schemas.ts",
    "db:fix-tenant-id": "ts-node db/fix-tenant-id.ts",
    "db:admin": "ts-node db/setup-admin.ts",
    "db:seed-admin": "ts-node db/seed-admin.ts",
    "db:verify-admin": "ts-node db/verify-admin.ts",
    "db:test-admin-login": "ts-node db/test-admin-login.ts",
    "db:validation": "ts-node db/apply-validation-triggers.ts",
    "db:seed": "ts-node db/fixed-seed.ts",
    "db:check-sales": "ts-node db/check-sales-schema.ts",
    "db:check-creditors": "ts-node db/check-creditors-schema.ts",
    "db:check-payments": "ts-node db/check-payments-schema.ts",
    "db:seed-simple": "ts-node db/simple-seed.ts",
    "db:verify-seed": "ts-node db/verify-seed.ts",
    "db:fix": "ts-node db/fix-all-relationships.ts",
    "db:seed-tenant-users": "ts-node db/seed-tenant-users.ts",
    "db:seed-all": "ts-node db/seed-all.ts",
    "db:setup": "ts-node db/setup-db.ts && ts-node db/fixed-seed.ts && ts-node db/fix-tenant-id.ts && ts-node db/setup-admin.ts && ts-node db/seed-admin.ts && ts-node db/apply-validation-triggers.ts && ts-node db/seed-simple.ts && ts-node db/fix-all-relationships.ts",
    "db:reset": "ts-node db/setup-db.ts && ts-node db/fixed-seed.ts && ts-node db/fix-tenant-id.ts && ts-node db/setup-admin.ts && ts-node db/seed-admin.ts && ts-node db/apply-validation-triggers.ts && ts-node db/seed-simple.ts && ts-node db/fix-all-relationships.ts",
    "db:migrate": "ts-node db/migrate.ts",
    "db:run-migrations": "ts-node db/run-migrations.ts",
    "db:rollback": "ts-node db/migrate.ts --rollback",
    "db:status": "ts-node db/migrate.ts --status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "generate-docs": "swagger-jsdoc -d ./swagger.json -o ./openapi.json"
  },
  "dependencies": {
    "@types/compression": "^1.8.1",
    "@types/helmet": "^0.0.48",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^6.2.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "swagger-ui-express": "^4.6.3",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.34",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "author": "",
  "license": "ISC"
}