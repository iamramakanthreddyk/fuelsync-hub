{
  "name": "fuelsync-hub-backend",
  "version": "1.0.0",
  "description": "Backend for FuelSync Hub - Multi-tenant Fuel Station Management Platform",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "db:setup": "ts-node db/setup-db.ts && ts-node db/seed.ts && ts-node db/fix-relationships.ts",
    "db:reset": "ts-node db/reset-db.ts && ts-node db/setup-db.ts && ts-node db/seed.ts && ts-node db/fix-relationships.ts",
    "db:clean": "ts-node db/reset-db.ts",
    "db:check": "ts-node db/check-connection.ts",
    "db:check-admin": "ts-node db/check-admin.ts",
    "db:check-data": "ts-node db/check-data.ts",
    "db:check-tables": "ts-node db/check-tables.ts",
    "db:fix": "ts-node db/fix-relationships.ts",
    "db:verify": "ts-node db/verify-seed.ts",
    "test": "jest --runTestsByPath src/tests/sanity.test.ts src/tests/nozzle.test.ts",
    "test:watch": "jest --watch src/tests/sanity.test.ts src/tests/nozzle.test.ts",
    "test:coverage": "jest --coverage src/tests/sanity.test.ts src/tests/nozzle.test.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix"
    },
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^6.2.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.10.0",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^18.16.3",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.16",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "author": "",
  "license": "ISC"
}